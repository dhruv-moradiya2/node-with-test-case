{
  "name": "jest-coverage-demo",
  "version": "1.0.0",
  "description": "Demo Node.js app with Jest coverage and GitHub Actions",
  "main": "server.js",
  "scripts": {
    "test": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0",
    "mongodb": "^6.20.0"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "reports/junit",
          "outputName": "jest-junit.xml"
        }
      ]
    ]
  }
}
